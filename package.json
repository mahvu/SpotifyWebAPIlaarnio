{
  "name": "spotifywebapilaarnio",
  "version": "1.0.0",
  "description": "A small application for fetching data through Spotify's Web API",
  "main": "app.js",
  "scripts": {
    "start": "better-npm-run start",
    "build": "better-npm-run build",
    "dev": "better-npm-run dev"
  },
  "betterScripts": {
    "build": {
      "command": "webpack --config webpack/prod.config.js --progress --colors",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start": {
      "command": "node server/app.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "dev": {
      "command": "node server/app.js",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mahvu/SpotifyWebAPIlaarnio.git"
  },
  "keywords": [
    "spotify"
  ],
  "author": "Lauri Rasi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mahvu/SpotifyWebAPIlaarnio/issues"
  },
  "homepage": "https://github.com/mahvu/SpotifyWebAPIlaarnio#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "node-sass": "^4.7.2",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.6",
    "webpack": "^3.8.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.9.4",
    "webpack-hot-middleware": "^2.20.0"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "better-npm-run": "^0.1.0",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "css-loader": "^0.28.7",
    "express": "^4.16.2",
    "foundation-sites": "^6.4.4-rc1",
    "if-env": "^1.0.0",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "morgan": "^1.9.0",
    "querystring": "^0.2.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-router-dom": "^4.2.2",
    "react-transform-hmr": "^1.0.4",
    "request": "^2.83.0",
    "spotify-web-api-node": "^2.5.0",
    "style-loader": "^0.19.0"
  }
}
